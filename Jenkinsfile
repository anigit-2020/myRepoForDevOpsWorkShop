pipeline {
   agent any

   stages {
      stage('Hello') {
         steps {
            echo 'Hello World'
         }
       stage('Stage #1') {
         steps {
            echo 'Your are in Stage #1'
         }
       stage('Stage #2') {
         steps {
            echo 'Your are in Stage #2'
         }
       stage('End Stage') {
         steps {
            echo 'Good Bye'
         }
      }
   }
}
